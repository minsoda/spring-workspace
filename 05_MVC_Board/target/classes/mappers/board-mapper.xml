<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">

<!-- useGeneratedKeys 런 할때마다 하나씩 추가 됨. keyProperty 가 no -->
	<insert id="insert" parameterType="Board" useGeneratedKeys="true" keyProperty="no" keyColumn="no">
		INSERT INTO board (no, title, content, writer)
		VALUES(SEQ_BOARD.NEXTVAL, #{title}, #{content}, #{writer})
	</insert>
	
	<select id="selectAll" resultType="Board" >
		SELECT no, title, content, writer, regdate
		FROM board
</select>

	<select id="select" resultType="Board">
		SELECT no, title, content, writer
		FROM board
		WHERE 
	</select>
	
	<update id="update" parameterType="Board">
		UPDATE board
		SET
		WHERE
	</update>
	
	<delete id="delete" parameterType="Board">
		DELETE 
		FROM board
		WHERE 
	</delete>
		
</mapper>